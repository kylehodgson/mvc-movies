@model MvcMovies.Models.ViewModels.SearchResultViewModel

@{
    ViewBag.Title = "SearchResults";
}

@{ Html.RenderAction("SearchForm"); }

<h2>SearchResults</h2>

@foreach (var movie in Model.Movies)
{
    <div>
        @Html.DisplayFor(m => movie.Title)
        @Html.ActionLink("View", "Details", new { id = movie.Id})
    </div>
}